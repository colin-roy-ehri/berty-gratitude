  "utilities": {
    "code_lookup_function": "function lookupCode(code) { /* Traverse hierarchy using code digits */ }",
    "validation": {
      "commodity_length": 8,
      "format": "digits only",
      "must_be_paired_with": "MessageType (REQUEST or RESPONSE)"
    },
    "restricted_segments": [
      "82 (Legal Services)",
      "83 (Professional Education)",
      "84 (Financial Services)",
      "85 (Healthcare Services)",
      "90 (Commercial Childcare)"
    ],
    "note": "Restricted segments can appear in REQUEST messages only (seeking navigation help), never in OFFER messages"
  }
