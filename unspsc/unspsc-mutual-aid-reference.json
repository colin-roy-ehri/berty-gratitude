{
  "metadata": {
    "title": "UNSPSC Codes for Mutual Aid",
    "version": "1.0",
    "last_updated": "2025-12-06",
    "purpose": "Machine-readable reference for mutual aid app classification",
    "format": "JSON",
    "hierarchy": "segment (2) → family (4) → class (6) → commodity (8)",
    "notes": "Codes optimized for peer-to-peer community support, excludes professional services"
  },
  "hierarchy_explanation": {
    "segment": "2-digit code, broadest category (e.g., 50 = Food)",
    "family": "4-digit code, product family (e.g., 5020 = Food Products)",
    "class": "6-digit code, specific class (e.g., 502015 = Fresh Fruits & Vegetables)",
    "commodity": "8-digit code, most specific item (e.g., 50201506 = Fresh Vegetables)"
  },
